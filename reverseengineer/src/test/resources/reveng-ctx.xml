<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
  
    <!-- RevengConfig -->
    <bean id="revengConfig" class="net.sourceforge.greenvine.reveng.impl.RevengConfig">
        <property name="modelName" value="test" />
    </bean>
    
    <!-- ReverseEngineer -->
    <bean id="reverseEngineer" class="net.sourceforge.greenvine.reveng.impl.ReverseEngineerImpl">
        <constructor-arg><ref bean="revengConfig" /></constructor-arg>
    </bean>
     
</beans>